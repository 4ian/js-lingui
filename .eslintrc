{
  "env": {
    "jest/globals": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "plugin:flowtype/recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
  ],
  "plugins": [
    "promise",
    "flowtype",
    "import",
    "react",
    "jest",
    "prettier"
  ],
  "rules": {
    "no-duplicate-imports": "off",
    "react/display-name": "off",
    "prettier/prettier": "error"
  }
}
