exports[`example-usecase should render 1`] = `
<Usecase>
  <I18nProvider
    messages={
      Object {
        "Hello World": "Ahoj světe",
        "My name is {name}": "Jmenuji se {name}",
        "msg.label": "Label",
      }
    }>
    <div>
      <InjectI18n
        className="untranslated"
        id="This isn\'t translated">
        <Trans
          className="untranslated"
          i18n={
            Object {
              "language": undefined,
              "messages": Object {
                "Hello World": "Ahoj světe",
                "My name is {name}": "Jmenuji se {name}",
                "msg.label": "Label",
              },
            }
          }
          id="This isn\'t translated">
          <span
            className="untranslated">
            This isn\'t translated
          </span>
        </Trans>
      </InjectI18n>
      <InjectI18n
        className="customId"
        defaults=""
        id="msg.label">
        <Trans
          className="customId"
          defaults=""
          i18n={
            Object {
              "language": undefined,
              "messages": Object {
                "Hello World": "Ahoj světe",
                "My name is {name}": "Jmenuji se {name}",
                "msg.label": "Label",
              },
            }
          }
          id="msg.label">
          <span
            className="customId">
            Label
          </span>
        </Trans>
      </InjectI18n>
      <InjectI18n
        className="translated"
        id="Hello World">
        <Trans
          className="translated"
          i18n={
            Object {
              "language": undefined,
              "messages": Object {
                "Hello World": "Ahoj světe",
                "My name is {name}": "Jmenuji se {name}",
                "msg.label": "Label",
              },
            }
          }
          id="Hello World">
          <span
            className="translated">
            Ahoj světe
          </span>
        </Trans>
      </InjectI18n>
      <InjectI18n
        className="variable"
        id="My name is {name}"
        params={
          Object {
            "name": "Mononoke",
          }
        }>
        <Trans
          className="variable"
          i18n={
            Object {
              "language": undefined,
              "messages": Object {
                "Hello World": "Ahoj světe",
                "My name is {name}": "Jmenuji se {name}",
                "msg.label": "Label",
              },
            }
          }
          id="My name is {name}"
          params={
            Object {
              "name": "Mononoke",
            }
          }>
          <span
            className="variable">
            Jmenuji se Mononoke
          </span>
        </Trans>
      </InjectI18n>
      <InjectI18n
        className="components"
        components={
          Array [
            <a
              href="/mononoke" />,
          ]
        }
        id="Read <0>more</0>.">
        <Trans
          className="components"
          components={
            Array [
              <a
                href="/mononoke" />,
            ]
          }
          i18n={
            Object {
              "language": undefined,
              "messages": Object {
                "Hello World": "Ahoj světe",
                "My name is {name}": "Jmenuji se {name}",
                "msg.label": "Label",
              },
            }
          }
          id="Read <0>more</0>.">
          <span
            className="components">
            Read 
            <a
              href="/mononoke">
              more
            </a>
            .
          </span>
        </Trans>
      </InjectI18n>
    </div>
  </I18nProvider>
</Usecase>
`;
