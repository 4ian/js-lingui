exports[`example-usecase should render 1`] = `
<Usecase>
  <I18nProvider
    language="cs"
    messages={
      Object {
        "Hello World": "Ahoj světe",
        "My name is {name}": "Jmenuji se {name}",
        "msg.label": "Nápis",
      }
    }>
    <NeverUpdate>
      <div>
        <InjectI18n
          className="untranslated"
          id="This isn\'t translated">
          <Trans
            className="untranslated"
            i18n={
              Object {
                "language": "cs",
                "translate": [Function],
              }
            }
            id="This isn\'t translated">
            <span
              className="untranslated">
              This isn\'t translated
            </span>
          </Trans>
        </InjectI18n>
        <InjectI18n
          className="customId"
          defaults=""
          id="msg.label">
          <Trans
            className="customId"
            defaults=""
            i18n={
              Object {
                "language": "cs",
                "translate": [Function],
              }
            }
            id="msg.label">
            <span
              className="customId">
              Nápis
            </span>
          </Trans>
        </InjectI18n>
        <InjectI18n
          className="translated"
          id="Hello World">
          <Trans
            className="translated"
            i18n={
              Object {
                "language": "cs",
                "translate": [Function],
              }
            }
            id="Hello World">
            <span
              className="translated">
              Ahoj světe
            </span>
          </Trans>
        </InjectI18n>
        <InjectI18n
          className="variable"
          id="My name is {name}"
          params={
            Object {
              "name": "Mononoke",
            }
          }>
          <Trans
            className="variable"
            i18n={
              Object {
                "language": "cs",
                "translate": [Function],
              }
            }
            id="My name is {name}"
            params={
              Object {
                "name": "Mononoke",
              }
            }>
            <span
              className="variable">
              Jmenuji se Mononoke
            </span>
          </Trans>
        </InjectI18n>
        <InjectI18n
          className="components"
          components={
            Array [
              <a
                href="/mononoke" />,
            ]
          }
          id="Read <0>more</0>.">
